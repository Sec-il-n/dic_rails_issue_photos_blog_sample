<h1>Log in</h1>
<div class="login_form">
  <%= form_with(scope: :session,url: sessions_path, local: true) do |f| %>
    <p><%= f.label :email %></p>
    <%= f.email_field :email %>
    <p><%= f.label :password %></p>
    <%= f.password_field :password %><br>
    <%= f.submit 'Log in' %><br>
  <% end %>
</div>
